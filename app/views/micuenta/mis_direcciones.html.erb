<%= content_for :contenido do %>
   
   <div class="text-right" style="margin-bottom:10px">
      <a class="btn btn-info">Registrar</a>
   </div>
   
   <table class="table table-bordered table-hover">
    <thead>
     <tr>
        <th>Distrito</th>
        <th>Direccion</th>
        <th></th>
     </tr>
    </thead>
    
    <tbody>
       <% @streets.each do |s| %>
      <tr>
        <td><%= reservation.created_at.strftime("%d/%m/%Y %I:%M%p") %></td>
        <td><%= reservation.origin_name %></td>
        <td><%= reservation.destination_name %></td>
        <td> <span class="label label-warning"><%= reservation.status.text %></span>  </td>
        <td>S./ <%= reservation.price %></td>
        <td><%= reservation.qualification %></td>
        <td> <a class="btn btn-default btn-xs">Detalle</a> <a class="btn btn-success btn-xs">Calificar</a> </td>
      </tr>
       <% end %>
    </tbody>
   </table>
   
<% end %>

<%= render template: "micuenta/laycuenta" %>