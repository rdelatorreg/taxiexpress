<%- model_class = Mystreet -%>
<%= content_for :contenido do %>
<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:district_id) %>:</strong></dt>
  <dd><%= @mystreet.district_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:street) %>:</strong></dt>
  <dd><%= @mystreet.street %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              mystreets_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_mystreet_path(@mystreet), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              mystreet_path(@mystreet),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>

<% end %>

<%= render template: "micuenta/laycuenta" %>